<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="797.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProfileController">
   <children>
      <ImageView fx:id="profImage" fitHeight="805.0" fitWidth="628.0" layoutX="-3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/profiles/profile2.jpg" />
         </image>
      </ImageView>
      <Label fx:id="nameLabel" alignment="TOP_LEFT" layoutX="37.0" prefHeight="28.0" prefWidth="506.0" text="здесь будут настройки">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="expLabel" alignment="TOP_LEFT" layoutX="368.0" layoutY="625.0" prefHeight="147.0" prefWidth="163.0" text="имя" wrapText="true" />
      <Label fx:id="specLabel" layoutX="368.0" layoutY="604.0" prefHeight="18.0" prefWidth="163.0" text="специальность:" />
      <Label fx:id="contLabel" layoutX="50.0" layoutY="177.0" prefHeight="26.0" prefWidth="455.0" text="контакты:" wrapText="true" />
      <Label fx:id="aboutLabel" alignment="TOP_LEFT" layoutX="366.0" layoutY="413.0" prefHeight="166.0" prefWidth="170.0" text="Label" textAlignment="JUSTIFY" wrapText="true" />
      <Label fx:id="ageLabel" alignment="TOP_LEFT" layoutX="50.0" layoutY="138.0" prefHeight="49.0" prefWidth="455.0" text="Возраст: " wrapText="true" />
      <TextField fx:id="contText" editable="false" layoutX="50.0" layoutY="201.0" style="-fx-background-color: transparent; -fx-background-insets: 0px;" />
   </children>
</AnchorPane>
